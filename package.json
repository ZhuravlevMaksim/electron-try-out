{
  "name": "electron-try-out",
  "author": "m.zhuravlev",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/react": "^17.0.0",
    "@vitalets/google-translate-api": "^5.0.0",
    "epub": "^1.2.1",
    "node-sass": "4.14.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.1",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "build:electron": "electron-builder build --publish=never --win --ia32",
    "build:react:electron": "yarn build && yarn run build:electron"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "react-router-dom": "^5.2.0",
    "electron-builder": "22.3.6",
    "electron": "^11.1.1"
  },
  "main": "public/main.js",
  "homepage": "./",
  "build": {
    "productName": "oracion",
    "appId": "electron-try-out-desktop",
    "extends": null,
    "extraMetadata": {
      "main": "build/main.js"
    },
    "files": [
      "build/"
    ],
    "win": {
      "artifactName": "${name}-win-${version}.${ext}",
      "target": [
        "dir"
      ]
    }
  }
}
